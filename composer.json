{
  "name": "iluckhack/xhprof-buggregator-bundle",
  "description": "Xhprof buggregator integration for Symfony Framework",
  "keywords": [
    "iluckhack",
    "symfony",
    "buggregator",
    "xhprof"
  ],
  "homepage": "https://github.com/iluckhack/xhprof-buggregator-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrey Pakhomov",
      "email": "luckhack@inbox.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "spiral-packages/profiler": "^1.0",
    "symfony/console": ">=6.4",
    "symfony/config": ">=6.4",
    "symfony/dependency-injection": ">=6.4",
    "symfony/event-dispatcher": ">=6.4",
    "symfony/http-kernel": ">=6.4"
  },
  "autoload": {
    "psr-4": {
      "Iluckhack\\XhprofBuggregatorBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Iluckhack\\XhprofBuggregatorBundle\\Tests\\App\\": "tests/project/src",
      "Iluckhack\\XhprofBuggregatorBundle\\Tests\\Integration\\": "tests/Integration",
      "Iluckhack\\XhprofBuggregatorBundle\\Tests\\Helper\\": "tests/Helper"
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "symfony/test-pack": "^1.1",
    "symfony/framework-bundle": ">=6.4",
    "symfony/dotenv": ">=6.4",
    "symfony/yaml": ">=6.4"
  },
  "scripts": {
    "tests": "./vendor/bin/simple-phpunit"
  }
}
